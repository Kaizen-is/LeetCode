        "import pandas as pd\n",
        "\n",
        "def find_customer_referee(customer: pd.DataFrame) -> pd.DataFrame:\n",
        "    res = customer[(customer['referee_id'].isnull()) | (customer['referee_id'] != 2)]\n",
        "    return res[['name']]"
      ]
